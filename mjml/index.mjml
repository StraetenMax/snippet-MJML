
<mjml>
  <mj-head>
    <mj-title>Snippets Mjml</mj-title>
    <mj-raw>
      <meta name="color-scheme" content="light"/>
      <meta name="supported-color-schemes" content="light"/>
      <meta name="format-detection" content="telephone=no"/>
      <meta name="format-detection" content="date=no"/>
      <meta name="format-detection" content="address=no"/>
      <meta name="format-detection" content="email=no"/>
      
    </mj-raw>
    <mj-breakpoint width="480px"></mj-breakpoint>
    <mj-font name="Inter" href="https://fonts.googleapis.com/css2?family=Inter:wght@100..900&amp;display=swap"/>
    <mj-font name="Montserrat" href="https://fonts.googleapis.com/css?family=Montserrat"></mj-font>
    <mj-style>
      .space{
          padding: 30px !important;
      }
      .card {
          border: 1px solid red !important;
          border-radius: 6px !important;
      }
      .int{
          padding: 20px !important;
      }
      .button{
          background-color: #016ECC;
          border: none;
          border-radius: 24px;
          color: #FFFFFF;
          padding: 9px 20px;
          text-align: center;
          text-decoration: none;
          display: inline-block;
          font-size: 13px;
          font-weight:600;
      }
      .separate table{
          border-collapse: separate;
      }
      @media only screen and (max-width:480px) {
          .space {
              padding: 10px !important;
          }
          .int{
              padding:10px !important;
          }
      }
    </mj-style>
  </mj-head>
